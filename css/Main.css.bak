#Main{
	min-width=1250px;
	height:500px;
	background-color:yellow;
}
#Main .Main-nav{
	position:relative;
	font-size:12px;
	color:white;
	width:1220px;
	height:500px;
	margin:auto;
}

#Main .Main-nav a{
	color:white;
	color:inherit;
}

#Main .Main-nav .Main-nav-category{
	z-index:2;
	position:absolute;
	top:-37px;
	left:-7px;
	width:200px;
	height:537px;
	background:rgba(0,0,0,0.5);
}

#Main .Main-nav .Main-nav-category h2 i{
	padding:10px;
	font-size:18px;
	vertical-align:top;
}
#Main .Main-nav .Main-nav-category h2{
	line-height:37px;
	background-color:#FF0036;
	vertical-align:top;
}

#Main .Main-nav .Main-nav-category ul li{
	line-height:30px;
}
#Main .Main-nav .Main-nav-category ul li>a{
	vertical-align:top;
}
#Main .Main-nav .Main-nav-category ul li>i{
	padding:10px;
	font-weight:normal;
}

#Main .Main-nav .Main-nav-category>ul>li:hover{
	font-weight:600;
	background-color:white;
}

#Main .Main-nav .Main-nav-category>ul>li:hover .hide{
	display:block;
}
#Main .Main-nav .Main-nav-category>ul>li .hide{
	display:none;
	color:#999;
}

#Main .Main-nav .Main-nav-category .hide{
	position:absolute;
	top:37px;
	right:-855px;
	width:855px;
	height:503px;
	background-color:#FAFAFA;
}

#Main .Main-nav .Main-nav-category .hide .category-left{
	width:643px;
	background-color:#FFF;
}

#Main .Main-nav .Main-nav-category .hide .category-left .Sub-category{
	width:100%;
	padding-left:30px;
}

#Main .Main-nav .Main-nav-category .hide .category-left .Sub-category h4{
	margin-top:10px;
}

#Main .Main-nav .Main-nav-category .hide .category-left .Sub-category ul{
	padding:14px;
	margin-left:14px;
	width:470px;
	border-bottom: 1px dashed #999; 
}

#Main .Main-nav .Main-nav-category .hide .category-left .Sub-category ul li{
	line-height:22px;
	margin-right:12px;
}


#Main .Main-nav .Main-nav-category .hide .category-right{
	width:192px;
	padding:10px;
	background-color:#FAFAFA;
}

#Main .Main-nav .Main-nav-category .hide .category-right>ul>li{
	float:left;
	width:94px;
	height:45px;
	margin-bottom:1px;
	margin-right:1px;
}

#Main .Main-nav .Main-nav-category .hide .category-right>ul>li a{
	display:block;
	height:100%;
}

#Main .Main-nav .Main-nav-category .hide .category-right>ul>li a img{
	height:100%;
	width:100%;
}



