#Main .Main-nav .Main-nav-img{
	position:relative;
	width:1230px;
	height:500px;
	top:-32px;
	left:0;
	background-color:lightblue;
}

#Main .Main-nav .Main-nav-img .display{
	position:absolute;
	z-index:1;
	right:500px;
	bottom:26px;
}

#Main .Main-nav .Main-nav-img .display>label{
	float:left;
	width:25px;
	height:5px;
	margin-left:9px;
	background-color:#999;
}

#Main .Main-nav .Main-nav-img .display .image{
	position:absolute;
	z-index:-1;
	left:-533px;
	top:-469px;
	display:none;
	width:1230px;
	height:500px;
}


#Main .Main-nav .Main-nav-img .display .image>a{
	width:100%;
	height:100%;
}

#Main .Main-nav .Main-nav-img .display .image>a>img{
	width:100%;
	height:100%;
}

#Main .Main-nav .Main-nav-img .display input:checked+label{
	background-color:#F6F6F6;
}

#Main .Main-nav .Main-nav-img .display input:checked+label+.image{
	display:block;
}

#Main .Main-nav .Main-nav-img .display input[type=radio]{
			display:none;
}

#Main .Main-nav .Main-nav-img .display .image:before{
	content:'';
	width:160%;
	height:100%;
	display:block;
	position:absolute;
	z-index:-1;
	left:-340px;
}

#Main .Main-nav .Main-nav-img .display input:checked+.label1+.image:before{
	background-color:#E8E8E8;
}

#Main .Main-nav .Main-nav-img .display input:checked+.label2+.image:before{
	background-color:#FE5DFB;
}
#Main .Main-nav .Main-nav-img .display input:checked+.label3+.image:before{
	background-color:#E8E8E8;
}
#Main .Main-nav .Main-nav-img .display input:checked+.label4+.image:before{
	background-color:#D9123D;
}
#Main .Main-nav .Main-nav-img .display input:checked+.label5+.image:before{
	background-color:#6B4A24;
}
#Main .Main-nav .Main-nav-img .display input:checked+.label6+.image:before{
	background-color:#0235D4;
}


#Main .Main-nav .Main-Header{
	width:1000px;
	height:32px;
	color:#333;
	font-size:15px;
	position:relative;
	left:193px;
	top:-34px;
}

#Main .Main-nav .Main-Header ul li{
	float:left;
	line-height:32px;
	padding:0px 10px;
}
#Main .Main-nav .Main-Header ul li>a>img{
	width:100%;
	height:100%;
}
#Main .Main-nav .Main-Header ul li:hover>a{
	color:#FF0036;
}

#Main .Main-nav .Main-Header ul li:nth-child(1)>a{
	display:block;
	width:105px;
}
#Main .Main-nav .Main-Header ul li:nth-child(2)>a{
	display:block;
	width:105px;
}

#Main .Main-nav .Main-Header ul li:nth-child(n+3){
	display:block;
}